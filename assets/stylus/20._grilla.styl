// Estructura general de la web
.cuerpo
	background texto_header
	position relative

.cuerpo__main
	margin-top 69px
	padding 0.8rem
	position relative



/* Modo Desktop */
@media all and (min-width 1090px)
	.main
		align-content center
		display flex
		justify-content space-between
		max-width 1072px
		margin-left auto
		margin-right auto
		margin-top 1rem

	.cuerpo__main
		margin-top 100px
